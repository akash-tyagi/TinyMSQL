CREATE TABLE r (a INT, b INT)
CREATE TABLE s (b INT, c INT)
CREATE TABLE t (a INT, c INT)
CREATE TABLE u (c INT, d INT)
INSERT INTO r (a, b) VALUES (0, 9)
INSERT INTO r (a, b) VALUES (1, 8)
INSERT INTO s (b, c) VALUES (10, 20)
INSERT INTO s (b, c) VALUES (12, 22)
INSERT INTO t (a, c) VALUES (102, 102)
INSERT INTO t (a, c) VALUES (202, 102)
INSERT INTO t (a, c) VALUES (302, 102)
INSERT INTO u (c, d) VALUES (3020, 102)
INSERT INTO u (c, d) VALUES (3025, 102)
SELECT r.a, u.c FROM r, u ORDER BY u.c